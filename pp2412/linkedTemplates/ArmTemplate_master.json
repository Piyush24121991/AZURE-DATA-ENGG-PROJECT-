			"type": "string",
			"defaultValue": "Piyush24121991"
		},
		"localfiles_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Program Files\\data store"
		},
		"localfiles_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Piyush Pawar"
