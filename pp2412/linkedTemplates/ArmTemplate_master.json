					"localfiles_properties_typeProperties_host": {
						"value": "[parameters('localfiles_properties_typeProperties_host')]"
					},
					"localfiles_properties_typeProperties_userId": {
						"value": "[parameters('localfiles_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'pp2412_ArmTemplate_0')]"
			]
		},
		{
			"name": "pp2412_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
